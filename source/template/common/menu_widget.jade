.main-menu-box
  .main-menu-box__left
  .main-menu-box__right
  .main-menu-box__content
    -
      var items = {
      "МОИ РАБОТЫ"  : "works.html",
      "БЛОГ"        : "blog.html",
      "ОБО МНЕ"     : "about.html",
      "АВТОРИЗАЦИЯ" : "index.html"
      }
    +setList(items, "main-menu")
.menu-hamburger
  .menu-hamburger__item#menu-hamburger__item--line-first
  .menu-hamburger__item#menu-hamburger__item--line-second
  .menu-hamburger__item#menu-hamburger__item--line-third
//a.menu__link.link-humburger(href="#")
  svg.menu__svg.menu__svg--humburger(preserveAspectRatio="xMinYMin meet", viewBox="0 0 40 35")
    title.svg__title menu
    use.svg__icon(xlink:href="assets/img/sprite/img.svg#hamburger")

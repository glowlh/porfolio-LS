header.header
  .night
    img.night__img(src="/assets/img/background-night-big.jpg")
  .header__content
    .header__widgets.clearfix
      .widgets__socials
        include socials_widget
      .widgets__menu
        include menu_widget
    .header__img
      svg.stars__svg
        use.svg__icon(xlink:href="assets/img/sprite/img.svg#stars1")
      .photo
        +setRoudedImage("/assets/img/welcome-face.jpg", "Фото Гущмной Марии")

  block header__title